{
  "name": "react-ssr-slow-request",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production nodemon server.js",
    "build:client": "vite build --outDir dist/client",
    "watch:client": "vite build --watch --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.js",
    "watch:server": "vite build --watch --outDir dist/server --ssr src/entry-server.js"
  },
  "devDependencies": {
    "@types/express": "^5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-ssr": "^0.4.142",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
